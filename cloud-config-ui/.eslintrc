env:
  browser: true
  es6: true

ecmaFeatures:
  jsx: true

parser: "babel-eslint"

rules:
  no-undef: 2
  no-undef-init: 1
  yoda: 1
  semi-spacing: 1
  radix: 1
  no-console: 0
  no-void: 1
  no-sequences: 1
  no-self-compare: 1
  no-return-assign: 1
  no-extra-bind: 1
  no-extend-native: 1
  no-empty-label: 1
  no-unused-expressions: 0
  no-alert: 1
  eqeqeq: 1
  dot-notation: 1
  curly: 1
  valid-typeof: 1
  use-isnan: 1
  no-unreachable: 1
  no-obj-calls: 1
  no-dupe-keys: 1
  no-cond-assign: 1
  no-unused-vars: 0
  no-underscore-dangle: 0
  consistent-return: 0
  comma-dangle: 0
  quotes: [1, "single", "avoid-escape"]
  global-strict: 0
  space-infix-ops: 1
  strict: 0
